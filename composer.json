{
  "name": "etki/allure-runner",
  "description": "Simple package providing API for allure-cli implementation",
  "authors": [
    {
      "name": "Etki",
      "email": "etki@etki.name",
      "role": "Developer"
    }
  ],
  "keywords": ["testing", "test", "allure", "codeception", "phpunit"],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Etki\\Testing\\AllureFramework\\Runner\\": "src/",
      "Etki\\Shortcut\\AllureRunner\\": "shortcuts/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etki\\Testing\\AllureFramework\\Runner\\Tests\\": "tests/Suites",
      "Etki\\Testing\\AllureFramework\\Runner\\Tests\\Support\\": "tests/Support"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "ext-mbstring": "*",
    "symfony/process": "~2.6",
    "symfony/filesystem": "~2.6",
    "symfony/yaml": "~2.6",
    "symfony/dependency-injection": "~2.6",
    "symfony/config": "~2.6",
    "symfony/validator": "~2.6",
    "guzzle/guzzle": "~3.9",
    "knplabs/github-api": "~1.2",
    "symfony/property-access": "~2.6",
    "doctrine/annotations": "~1.2"
  },
  "require-dev": {
    "php": ">=5.4.0",
    "codeception/codeception": "~2.0",
    "allure-framework/allure-codeception": "dev-master",
    "php-vfs/php-vfs": "~1.2",
    "ramsey/uuid": "~3.0@dev",
    "satooshi/php-coveralls": "dev-master",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "suggest": {
    "codeception/codeception": "Rich testing framework Allure runner provides support for",
    "sebastianbergmann/phpunit": "Industry standard PHP testing framework Allure runner provides support for",
    "allure-framework/allure-codeception": "Allure Framework integration extension for Codeception",
    "allure-framework/allure-phpunit": "Allure Framework integration listener for PHPUnit"
  },
  "config": {
    "bin-dir": "bin"
  }
}
